{
  "name": "mina-wasm-mvp",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel serve web/index.html --open --lazy",
    "watch:ext": "parcel watch chrome-ext/{background.ts,content_script.ts,*.html} --no-hmr",
    "patch-wasm": "node patch.js",
    "build:proto": "rimraf web/pb && node protogen.js",
    "build:wasm": "wasm-pack build -t nodejs --out-name wasm && yarn patch-wasm",
    "build:web": "rimraf dist/web && parcel build web/*.html --no-source-maps --dist-dir dist/web",
    "build:ext": "rimraf dist/ext && parcel build web/*.html --no-source-maps --dist-dir dist/ext && parcel build chrome-ext/{*.ts,*.html} --no-source-maps --dist-dir dist/ext",
    "build": "yarn build:wasm && yarn clean && yarn build:web && yarn build:ext",
    "clean": "rimraf dist",
    "test": "jest",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --fix --ignore-path .gitignore --ignore-pattern web/pb/**/* .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --fix --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "help": "parcel build -h"
  },
  "staticFiles": {
    "staticPath": "static"
  },
  "dependencies": {
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "protobufjs": "^6.11.2",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@parcel/transformer-raw": "^2.3.2",
    "@parcel/transformer-sass": "^2.3.2",
    "@parcel/transformer-vue": "^2.3.2",
    "@types/chrome": "latest",
    "@types/jest": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "eslint": "^8.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "jest": "^27.5.1",
    "parcel": "^2.3.2",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "path-browserify": "^1.0.1",
    "postcss-html": "^1.3.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "stylelint": "^14.6.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "util": "^0.12.4"
  }
}
