{
  "name": "mina-wasm-mvp",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel serve web/*.html --open",
    "watch:ext": "parcel watch chrome-ext/{background.ts,content_script.ts,*.html} --no-hmr",
    "build:proto": "rimraf web/pb && node protogen.js",
    "build:wasm": "wasm-pack build -t web --out-name wasm",
    "build:web": "rimraf dist/web && parcel build web/*.html --no-source-maps --dist-dir dist/web",
    "build:ext": "rimraf dist/ext && parcel build web/*.html --no-source-maps --dist-dir dist/ext && parcel build chrome-ext/{*.ts,*.html} --no-source-maps --dist-dir dist/ext",
    "build:ext-v2": "rimraf dist/ext-v2 && yarn build:ext && cp -r dist/ext dist/ext-v2 && rm dist/ext-v2/manifest.json && mv dist/ext-v2/manifest-v2.json dist/ext-v2/manifest.json",
    "build": "yarn build:wasm && yarn clean && yarn build:web && yarn build:ext && yarn build:ext-v2",
    "clean": "rimraf dist",
    "test": "jest",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --fix --ignore-path .gitignore --ignore-pattern web/pb/**/* .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --fix --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "help": "parcel build -h"
  },
  "staticFiles": {
    "staticPath": "static"
  },
  "dependencies": {
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.17",
    "node-fetch": "^2.6.7",
    "protobufjs": "^6.11.3",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@parcel/transformer-inline-string": "^2.6.0",
    "@parcel/transformer-raw": "^2.6.0",
    "@parcel/transformer-sass": "^2.6.0",
    "@parcel/transformer-vue": "^2.6.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/chrome": "latest",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.42",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "jest": "^28.1.1",
    "parcel": "^2.6.0",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "tailwindcss": "^3.1.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3",
    "util": "^0.12.4"
  }
}