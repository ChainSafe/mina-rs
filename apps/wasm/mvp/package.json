{
  "name": "mina-wasm-mvp",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "parcel web/index.html --open",
    "wasm-pack": "wasm-pack build -t nodejs --out-name wasm",
    "patch": "node patch.js",
    "build:web": "yarn clean && parcel build web/index.html --no-source-maps",
    "build": "yarn wasm-pack && yarn patch && yarn build:web",
    "clean": "rm -r dist || echo ''",
    "test": "echo 'placeholder for tests'",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --fix --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --fix --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "help": "parcel build -h"
  },
  "dependencies": {
    "events": "^3.3.0",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@parcel/transformer-raw": "^2.3.2",
    "@parcel/transformer-sass": "^2.3.2",
    "@parcel/transformer-vue": "^2.3.2",
    "eslint": "^8.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "parcel": "^2.3.2",
    "postcss-html": "^1.3.0",
    "sass": "^1.49.9",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended-vue": "^1.3.0",
    "typescript": "^4.6.2"
  }
}
