{
  "manifest_version": 3,
  "name": "Mina Web Node(MVP)",
  "description": "Web node of the mina blockchain (MVP version)",
  "version": "0.1",
  "action": {
    "default_title": "Mina Web Node(MVP)"
  },
  "background": {
    "service_worker": "background.js"
  },
  "sandbox":{
    "pages":["index.html"]
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "storage"
  ]
}
